{
  "name": "pet-finder",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "sync": "ts-node ./back-src/sync.ts",
    "dev:back": "nodemon --watch ./back-src/index.ts --exec ts-node ./back-src/index.ts",
    "dev:front": "parcel ./front-src/index.html",
    "build:back": "tsc",
    "build:front": "parcel build ./front-src/index.html",
    "build": "npm run build:back && npm run build:front",
    "start": "node ./back-dist/index.js"
  },
  "dependencies": {
    "@mapbox/search-js-web": "^1.0.0-beta.18",
    "@vaadin/router": "^1.7.5",
    "algoliasearch": "^4.22.1",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^3.1.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "parcel": "^2.10.3"
  }
}
